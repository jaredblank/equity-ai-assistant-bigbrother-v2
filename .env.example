# Big Brother AI Assistant v2 - Environment Configuration
# =======================================================
# WARNING: Never commit actual .env file to version control
# Copy this file to .env and fill in your actual values

# Server Configuration
NODE_ENV=development
PORT=3005
HOST=localhost

# SQL Server Database Configuration
DB_SERVER=your-sql-server-host
DB_PORT=1433
DB_NAME=EquityAI_BigBrother
DB_USER=your-db-username
DB_PASSWORD=your-db-password
DB_ENCRYPT=true
DB_TRUST_SERVER_CERTIFICATE=true
DB_CONNECTION_TIMEOUT=30000
DB_REQUEST_TIMEOUT=30000
DB_POOL_MIN=2
DB_POOL_MAX=10

# ElevenLabs API Configuration
ELEVENLABS_API_KEY=your-elevenlabs-api-key
ELEVENLABS_API_URL=https://api.elevenlabs.io/v1
ELEVENLABS_VOICE_ID=your-default-voice-id
ELEVENLABS_MODEL_ID=eleven_multilingual_v2
ELEVENLABS_STABILITY=0.75
ELEVENLABS_SIMILARITY_BOOST=0.75
ELEVENLABS_STYLE=0.0
ELEVENLABS_USE_SPEAKER_BOOST=true

# AI Configuration
AI_MODEL=gpt-4-turbo-preview
AI_TEMPERATURE=0.7
AI_MAX_TOKENS=1000
AI_SYSTEM_PROMPT_VERSION=v2.0
AI_CONVERSATION_MEMORY_LIMIT=20
AI_RESPONSE_TIMEOUT=30000

# Performance Monitoring
ENABLE_PERFORMANCE_LOGGING=true
LOG_LEVEL=info
LOG_MAX_FILES=30
LOG_MAX_SIZE=20m

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100
RATE_LIMIT_VOICE_WINDOW_MS=60000
RATE_LIMIT_VOICE_MAX_REQUESTS=10

# Security
CORS_ORIGINS=http://localhost:3000,http://localhost:3001
JWT_SECRET=your-jwt-secret-key-change-this
ENCRYPTION_KEY=your-32-character-encryption-key

# Real Estate Configuration
BROKER_LICENSE_NUMBER=your-broker-license
DEFAULT_MARKET_AREA=your-default-market
MLS_ACCESS_TOKEN=your-mls-token
PROPERTY_SEARCH_RADIUS=25

# Big Brother Compliance
COMPLIANCE_LEVEL=STRICT
AUDIT_LOGGING=true
DATA_RETENTION_DAYS=90
CONVERSATION_LOGGING=true
VOICE_SYNTHESIS_LOGGING=true

# Health Check Configuration
HEALTH_CHECK_INTERVAL=30000
HEALTH_CHECK_TIMEOUT=5000
ENABLE_DETAILED_HEALTH=true